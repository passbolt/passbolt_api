	setSelectionEnd=Element_proto.setselectionEnd,
	getReversed=Element_proto.getreversed,
	getTextContent=Node_proto.gettextContent,
	setTextContent=Node_proto.settextContent,
	_g1=Node_proto.g1,
	_g2=Node_proto.g2,
	_g4=Node_proto.g4,
