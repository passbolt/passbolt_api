<SCRIPT>
/*@requared window.Node.prototype = { ielt8 : true }*/
var 
	Node_proto=window.Node.prototype,
	Element_proto=window.Element.prototype,
	getChildren=Element_proto.getchildren,
	getClassList=Element_proto.getclassList,
	getLastElementChild=Element_proto.getlastElementChild,
	getFirstElementChild=Element_proto.getfirstElementChild,
	getNextElementSibling=Element_proto.getnextElementSibling,
	getPreviousElementSibling=Element_proto.getpreviousElementSibling,
