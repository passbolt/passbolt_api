msgstr "\"{0}\" nie jest prawidłowym identyfikatorem grupy dla filtra {1}."

msgid "\"{0}\" is not a valid parent id for filter {1}."
msgstr "\"{0}\" nie jest prawidłowym identyfikatorem nadrzędnym dla filtra {1}."

msgid "\"{0}\" is not a valid timestamp for filter {1}."
msgstr "\"{0}\" nie jest prawidłowym oznaczeniem czasu dla filtra {1}."

msgid "\"{0}\" is not a valid datetime for filter {1}."
msgstr "\"{0}\" nie jest prawidłowym oznaczeniem daty dla filtra {1}."

msgid "\"{0}\" is not a valid order."
msgstr "\"{0}\" nie jest prawidłowym poleceniem."

msgid "\"{0}\" is not in the list of allowed order."
msgstr "\"{0}\" nie znajduje się na liście dozwolonych poleceń."

msgid "\"{0}\" is not a valid contain value."
msgstr "\"{0}\" nie jest poprawną wartością treści."

msgid "undefined user agent"
msgstr "niezdefiniowany klient użytkownika"

msgid "Access restricted to administrators."
msgstr "Dostęp zastrzeżony dla administratorów."

msgid "Access restricted to unauthenticated users."
msgstr "Dostęp ograniczony dla nieuwierzytelnionych użytkowników."

msgid "The user should not be logged in."
msgstr "Użytkownik nie powinien być zalogowany."

msgid "This feature is not supported by your version of passbolt."
msgstr "Te funkcja nie jest obsługiwana w Twojej wersji usługi passbolt."

msgid "The resource identifier should be a valid UUID."
msgstr "Identyfikator zasobu powinien być prawidłowym UUID."

