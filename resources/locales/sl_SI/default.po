msgid "\"{0}\" is not a valid resource id for filter {1}."
msgstr "\"{0}\" ni veljaven ID vira za filter {1}."

msgid "\"{0}\" is not a valid group id for filter {1}."
msgstr "\"{0}\" ni veljaven ID skupine za filter {1}."

msgid "\"{0}\" is not a valid parent id for filter {1}."
msgstr "\\\"{0}\\\" ni veljaven nadrejeni ID za filter {1}."

msgid "\"{0}\" is not a valid timestamp for filter {1}."
msgstr "\"{0}\" ni veljaven časovni žig za filter {1}."

msgid "\"{0}\" is not a valid datetime for filter {1}."
msgstr "\"{0}\" ni veljaven datum-čas za filter {1}."

msgid "\"{0}\" is not a valid order."
msgstr "\"{0}\" ni veljavno razvrščanje."

msgid "\"{0}\" is not in the list of allowed order."
msgstr "\"{0}\" ni na seznamu dovoljenih razvrstitev."

msgid "\"{0}\" is not a valid contain value."
msgstr "\"{0}\" ni veljavna vrednost contain."

msgid "undefined user agent"
msgstr "nedefiniran uporabniški agent"

msgid "Access restricted to administrators."
msgstr "Dostop omejen na administratorje."

msgid "Access restricted to unauthenticated users."
msgstr "Dostop je omejen na neavtentificirane uporabnike."

msgid "The user should not be logged in."
msgstr "Uporabnik ne sme biti prijavljen."

msgid "This feature is not supported by your version of passbolt."
msgstr "Ta funkcija ni podprta v vaši različici Passbolt."

msgid "The resource identifier should be a valid UUID."
msgstr "Identifikator vira mora biti veljaven UUID."

msgid "The resource was marked as favorite."
msgstr "Vir je bil označen kot priljubljen."

msgid "The favorite id is not valid."
msgstr "ID priljubljenega ni veljaven."

