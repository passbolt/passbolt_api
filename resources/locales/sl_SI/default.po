msgid "You need to login to access this location."
msgstr "Za dostop do te lokacije se morate prijaviti."

msgid "The server verify token is missing or invalid."
msgstr "Žeton za preverjanje strežnika je pomanjkljiv ali neveljaven."

msgid "Decryption failed."
msgstr "Dešifriranje ni uspelo."

msgid "Failed to create token."
msgstr "Ustvarjanje žetona ni uspelo."

msgid "The user token result should be a valid UUID."
msgstr "Rezultat uporabniškega tokena mora biti veljaven UUID."

msgid "The user token result could not be found "
msgstr "Rezultata uporabniškega tokena ni bilo mogoče najti"

msgid "The OpenPGP server key defined in the config cannot be used to decrypt."
msgstr "OpenPGP strežniškega ključa, določenega v konfiguraciji, ni mogoče uporabiti za dešifriranje."

msgid "Could not import the user OpenPGP key."
msgstr "Uporabniškega OpenPGP ključa ni bilo mogoče uvoziti."

msgid "Invalid verify token type."
msgstr "Neveljaven tip žetona za preverjanje."

msgid "Invalid verify token format, "
msgstr "Nepravilna oblika žetona za preverjanje,"

msgid "sections are missing or using wrong delimiters."
msgstr "manjkajo ali so uporabljeni napačni razmejitveni znaki (delimiterji)."

msgid "the version numbers do not match."
msgstr "številke različic se ne ujemajo."

msgid "wrong version number."
msgstr "napačna številka različice."

msgid "it is not a UUID."
msgstr "to ni UUID."

msgid "wrong token data length."
msgstr "napačna dolžina podatkov v žetonu."

msgid "There is no user associated with this key."
msgstr "S tem ključem ni povezan noben uporabnik."

msgid "Page not found."
msgstr "Stran ni bila najdena."

msgid "Please use .json extension in URL or accept application/json."
msgstr "Uporabite .json v URL-ju ali sprejmite application/json."

msgid "The request data can not be empty."
msgstr "Podatki zahtevka ne smejo biti prazni."

msgid "You are successfully logged in."
msgstr "Uspešno ste prijavljeni."

msgid "You are successfully logged out."
msgstr "Uspešno ste se odjavili."

msgid "The logout route should only be accessed with POST method."
msgstr "Do odjave je mogoče dostopati samo s POST metodo."

msgid "The OpenPGP public key information was not found in config."
msgstr "Informacije o javnem ključu OpenPGP niso bile najdene v konfiguraciji."

msgid "The operation was successful."
msgstr "Operacija je bila uspešna."

msgid "The comment was successfully added."
msgstr "Komentar je bil uspešno dodan."

msgid "The comment was deleted."
msgstr "Komentar je bil izbrisan."

msgid "The comment id is not valid."
msgstr "ID komentarja ni veljaven."

msgid "The comment was successfully updated."
msgstr "Komentar je bil uspešno posodobljen."

msgid "Invalid order. \"{0}\" is not in the list of allowed order."
msgstr "Neveljavno razvrščanje. \"{0}\" ni na seznamu dovoljenih razvrstitev."

