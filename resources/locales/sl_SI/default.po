msgstr "Filter {0} ni veljaven."

msgid "\"{0}\" is not a valid value for filter {1}."
msgstr "\"{0}\" ni veljavna vrednost za filter {1}."

msgid "\"{0}\" is not a valid search filter."
msgstr "\"{0}\" ni veljaven iskalni filter."

msgid "\"{0}\" is not a valid search filter. It is not a UTF8 string."
msgstr "\"{0}\" ni veljaven iskalni filter. Ni UTF8 niz."

msgid "It should be between 1 and 64 char in length."
msgstr "Dolžina naj bo med 1 in 64 znaki."

msgid "\"{0}\" is not a valid user filter."
msgstr "\"{0}\" ni veljaven filter uporabnika."

msgid "\"{0}\" is not a valid user id for filter {1}."
msgstr "\"{0}\" ni veljaven ID uporabnika za filter {1}."

